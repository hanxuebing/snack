{
  "name": "snack",
  "private": true,
  "version": "0.1.0",
  "description": "snack，一个toB开发的工具合集，不提供过于精细的设置，但是提供开箱即用的功能，目前仅涵盖动画和公共方法",
  "packageManager": "pnpm@9.1.2",
  "main": "index.js",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "play": "turbo dev --filter=play",
    "dev": "turbo dev",
    "build": "turbo build",
    "build:ui": "turbo build --filter=@snack/ui",
    "build:calendar": "turbo build --filter=@snack/calendar",
    "preinstall": "npx only-allow pnpm",
    "storybook": "turbo storybook",
    "build:storybook": "turbo build-storybook",
    "build:vue3lib": "turbo build --filter=vue3lib",
    "clear-Node-modules": "node scripts/ClearNodeModules.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "sass": "^1.69.7",
    "turbo": "^1.11.2",
    "vite": "^5.0.10"
  },
  "engines": {
    "node": ">= 18"
  }
}
